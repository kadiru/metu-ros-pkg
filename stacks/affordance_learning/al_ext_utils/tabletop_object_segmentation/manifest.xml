<package>
  <description brief="tabletop_object_segmentation">
    Performs object segmentation and simple object recognition for
    constrained scenes.
  </description>

  <author>Marius Muja and Matei Ciocarlie</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/object_detector</url>

  <depend package="roscpp" />
  <depend package="tf"/>

  <depend package="visualization_msgs" />
  <depend package="sensor_msgs"/>
  <depend package="geometry_msgs" />

  <depend package="pcl"/>
  <depend package="pcl_ros"/>
  <depend package="pcl16"/>
  <depend package="pcl16_ros"/>
  <depend package="distance_field"/>
  <depend package="household_objects_database_msgs" />

  <depend package="al_msgs"/>

 <export>
    <cpp cflags="-I${prefix}/include -I${prefix}/msg_gen/cpp -I${prefix}/srv_gen/cpp" lflags="-Wl,-rpath,${prefix}/lib -L${prefix}/lib -lmarker_generator"/>
 </export>

</package>


